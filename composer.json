{
    "name": "qlimix/logging-exception-handler",
    "type": "library",
    "description": "Logging exceptions",
    "license": "MIT",
    "keywords": [
        "logging",
        "log",
        "exceptions"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "qlimix/logging": "^3.0"
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Log\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
